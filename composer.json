{
    "name": "anjola/php_rate_limiter",
    "description": "A PHP Library that seamlessly integrate limiting into your platform, supports Redis ,FileSystem , Datbases such as SQL Relational Database.",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "predis/predis": "^2.3",
        "ext-fileinfo": "*"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "PHPRateLimiter\\": "src/"
        }
    },
    "require-dev": {
        "php": "^7.2.5 || ^8.0",
        "phpoption/phpoption": "^1.9.3",
        "phpunit/phpunit": "^9.5",
        "bamarni/composer-bin-plugin": "^1.8.2",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpdocumentor/phpdocumentor": "^3.0"
    },
    "authors": [
        {
            "name": "Anjolaanuoluwapo",
            "email": "anjolaakinsoyinu@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
